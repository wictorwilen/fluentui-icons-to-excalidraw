{
  "name": "fluent-jot",
  "version": "1.0.0",
  "description": "Browse and download Microsoft's Fluent UI icons and emojis in hand-drawn Excalidraw format",
  "homepage": "https://fluentjot.design",
  "repository": {
    "type": "git",
    "url": "https://github.com/wictorwilen/fluentui-icons-to-excalidraw"
  },
  "keywords": [
    "fluent-jot",
    "fluent-ui",
    "icons",
    "emojis",
    "excalidraw",
    "hand-drawn",
    "sketching",
    "wireframe",
    "react",
    "typescript",
    "design-system"
  ],
  "author": "Wictor WilÃ©n",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm start",
    "build": "npm run prepare-data && npm run optimize-data && npm run optimize-emoji-data && npm run migrate-optimized && npm run migrate-emoji-optimized && react-scripts build",
    "prepare-data": "node build-scripts/prepare-data.js",
    "optimize-data": "node build-scripts/optimize-data.js",
    "optimize-emoji-data": "node build-scripts/optimize-emoji-data.js",
    "migrate-optimized": "node build-scripts/migrate-to-optimized.js",
    "migrate-emoji-optimized": "node build-scripts/migrate-emoji-to-optimized.js",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 3000",
    "start:swa": "swa start build",
    "build:swa": "npm run build && swa build",
    "deploy:swa": "swa deploy --env production",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "lint:check": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,css,json}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,json}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.6",
    "@headlessui/react": "^1.7.17",
    "clsx": "^2.0.0",
    "fuse.js": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.1.5",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  }
}
